<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean
		class="com.jspider.springcorexmdeplinjection.otm.bean.TeamBean"
		name="team1">
		<property name="id">
			<value>1</value>
		</property>
		<property name="name" value="Mumbai Indians" />
		<property name="city" value="Mumbai" />
		<property name="players">
			<list>
				<bean
					class="com.jspider.springcorexmdeplinjection.otm.bean.PlayerBean"
					name="player1">
					<property name="id">
						<value>1</value>
					</property>
					<property name="name" value="Rohit Sharma" />
					<property name="role" value="All-Rounder/Captun" />
					<property name="jsno" value="45" />
				</bean>
				<bean
					class="com.jspider.springcorexmdeplinjection.otm.bean.PlayerBean"
					name="player2">
					<property name="id">
						<value>2</value>
					</property>
					<property name="name" value="Ishan Kishan" />
					<property name="role" value="Wc/Batsman" />
					<property name="jsno" value="85" />
				</bean>
				
			</list>
			
		</property>
	</bean>
	
</beans>