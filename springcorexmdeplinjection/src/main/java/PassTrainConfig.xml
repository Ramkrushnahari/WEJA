<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.jspider.springcorexmdeplinjection.mto.bean.PassengerBean" name="passenger1">
		<property name="id">
			<value>1</value>
		</property>
		<property name="name" value="Sunita" />
		<property name="age" value="35" />
		<property name="train" ref="train1"/>
	</bean>
	<bean class="com.jspider.springcorexmdeplinjection.mto.bean.PassengerBean" name="passenger2">
		<property name="id">
			<value>2</value>
		</property>
		<property name="name" value="Prakash" />
		<property name="age" value="36" />
		<property name="train" ref="train1"/>
	</bean>
	
	<bean class="com.jspider.springcorexmdeplinjection.mto.bean.TrainBean" name="train1">
		<property name="id">
			<value>1</value>
		</property>
		<property name="name" value="Mumbai Express" />
		<property name="tno" value="654" />
		
	</bean>

</beans>